def entrega(a,b,c):
    # escreva aqui seu código
    return None


print("Example:")
print(entrega(1,2,2))

# As funções assert são utilizadas para verificar se o resultado da função chamada é o esperado
# testa a função com números positivos como parâmetros
assert entrega(1, 2, 2) == 2 
    
# testa a função com um número negativo como parâmetro
assert entrega(2, 2, 2) == 3
    
# testa a função com zero como parâmetro
assert entrega(1, 2, 3) == 1
    
print("Seu código passou pelos testes, clique em Check para avançar!")
